{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@framework/*": ["src/Framework/src/*"]
    }
  },
  "exclude": ["src/components/live2d/*", "src/Core/*", "src/Framework/src/*"]
}
